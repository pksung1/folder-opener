{
  "name": "@pksung1/folder-opener",
  "version": "0.0.5",
  "description": "Capacitor Folder Opener. The plugin is able to open a folder containing a specified file.",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "private": false,
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "Pksung1FolderOpener.podspec",
    "Package.swift"
  ],
  "author": "Seon Park",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pksung1/folder-opener.git"
  },
  "bugs": {
    "url": "https://github.com/pksung1/folder-opener/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "folder",
    "directory"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.mjs"
  },
  "devDependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/docgen": "^0.3.0",
    "@capacitor/ios": "^7.0.0",
    "@ionic/eslint-config": "^0.4.0",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^2.0.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.4.2",
    "prettier-plugin-java": "^2.6.6",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "swiftlint": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@capacitor/core": ">=7.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "android": {
      "src": "android"
    },
    "ios": {
      "src": "ios"
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "@ionic/prettier-config": {
      "@typescript-eslint/eslint-plugin": "$@typescript-eslint/eslint-plugin"
    }
  }
}
